<script type="text/javascript">
    var eth;
    var isTestnet = 'false';
    var SiteNetID = '56';
    var SiteNetChainID = '0x38';
    var SiteNetName = 'BNB Smart Chain Mainnet';
    var allowErc1155 = 'True';

        async function addNetwork(type) {

            if (type === 'web3') {
                if (typeof ethereum !== 'undefined') {
                    eth = new Web3(ethereum);
                } else if (typeof web3 !== 'undefined') {
                    eth = new Web3(web3.givenProvider);
                } else {
                    eth = new Web3(ethereum);
                }
            }

            if (typeof eth !== 'undefined') {
                var network = 0;
                network = await eth.eth.net.getId();
                netID = network.toString();
                var params;
                if (netID == SiteNetID) {
                      alert("BNB Smart Chain Network has already been added to Metamask.");
    return;
} else {
    params = [{
        chainId: SiteNetChainID,
        chainName: 'BNB Smart Chain Mainnet',
        nativeCurrency: {
            name: 'BNB',
            symbol: 'BNB',
            decimals: 18
        },
        rpcUrls: ['https://bsc-dataseed1.binance.org/'],
        blockExplorerUrls: ['https://bscscan.com']
}]
                }

                window.ethereum.request({ method: 'wallet_addEthereumChain', params })
                    .then(() => console.log('Success'))
                    .catch((error) => console.log("Error", error.message));
            } else {
                alert('Unable to locate a compatible web3 browser!');
            }
        }
    </script>
