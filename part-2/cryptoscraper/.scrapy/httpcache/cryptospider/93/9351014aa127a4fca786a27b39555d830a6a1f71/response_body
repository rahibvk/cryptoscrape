<script type="text/javascript">
        // INITIALIZATION OF HIGHLIGHT
        TextHighlight.init('bscscan');

        window.deposit_transfer_loaded = false;
        window.token_transfer_loaded = false;
        window.token_nft_transfer_loaded = false;
